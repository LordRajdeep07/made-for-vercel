{
  "version": 2,
  "functions": {
    "vercel/app.py": {
      "runtime": "python3.9",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "vercel/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 